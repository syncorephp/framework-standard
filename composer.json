{
   "name": "syncore/framework-standard",
   "type": "library",
   "description": "A PHP Framework libraries setup.",
   "authors": [{ "name": "Bartosz Zwski", "email": "duwless@outlook.com", "role": "Developer" }],
   "license": "MIT",
   "require": {
      "php": "^7.4 || ^8.0",
      "ext-json": "*",
      "ext-xml": "*",
      "ext-pdo": "*",
      "psr/container": "~v1.0",
      "psr/event-dispatcher": "~v1.0",
      "psr/log": "~v1.1",
      "symfony/yaml": "~v5.0",
      "twig/twig": "~v2.0 || ~v3.0",
      "vlucas/phpdotenv": "~v5.0"
   },
   "require-dev": {
      "flip/whoops": "~v2.9",
      "phpunit/phpunit": "~v9.5",
      "symfony/var-dumper": "~v5.0"
   },
   "autoload": {
      "psr-4": {
         "": "src/"
      }
   },
   "autoload-dev": {
      "psr-4": {
         "Tests\\": "tests/phpunit"
      }
   },
   "prefer-stable": true,
   "minimum-stability": "dev"
}
